{
    "docker": {
            "docker_cpu":{
                "class": "Gauge",
                "description": "total docker cpu usage",
                "default": -1
            },
            "docker_memory":{
                "class": "Gauge",
                "description": "total docker memory usage",
                "default": -1
            }
    },
    "physical": {
        "cpu_stat":{
            "cpu_percentage":{
                "class": "Gauge",
                "description": "monitor system cpu percentage",
                "default": -1,
                "key": "percentage"
            }
        },
        "mem_stat":{
            "memory_used": {
                "class": "Gauge",
                "description": "monitor system memory used",
                "default": 0,
                "key": "used"
            }
        },
        "proc_cpu":{
            "proc_cpu": {
                "class": "Gauge",
                "description": "monitor process CPU precentage",
                "default": 0,
                "key": "used"
            }
        },
        "proc_mem":{
            "proc_mem": {
                "class": "Gauge",
                "description": "monitor process memory precentage",
                "default": 0,
                "key": "used"
            }
        },
        "gpu":{
            "class": "Gauge",
            "description": "GPU usage",
            "default": -1
        },
        "network":{
            "class": "Gauge",
            "description": "network usage",
            "default": -1
        },
        "storage":{
            "class": "Gauge",
            "description": "Data storage",
            "default": -1
        }
    }
}